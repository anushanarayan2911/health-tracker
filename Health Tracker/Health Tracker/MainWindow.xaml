<Window x:Class="Health_Tracker.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Health_Tracker"
        mc:Ignorable="d"
        Title="HealthTracker" Height="450" Width="800">
    <Canvas>
        <Button x:Name="Food" Content="Food" Width="40" Height="20" Canvas.Left="20" Canvas.Top="20" VerticalAlignment="Top" HorizontalAlignment="Left" Click="FoodButtonClick"/>
        <Button x:Name="Exercise" Content="Exercise" Width="45" Height="20" Canvas.Left="20" Canvas.Top="60" VerticalAlignment="Top" HorizontalAlignment="Left" Click="ExerciseButtonClick"/>
        <Button x:Name="exerciseCalendar" Content="Exercise Calendar" Width="100" Height="20" Canvas.Left="20" Canvas.Top="100" VerticalAlignment="Top" HorizontalAlignment="Left" Click="ExerciseCalendarButtonClick"/>
        <Button x:Name="yourDay" Content="Your Day" Width="55" Height="20" Canvas.Left="20" Canvas.Top="140" VerticalAlignment="Top" HorizontalAlignment="Left" Click="YourDayButtonClick"/>
        <Button x:Name="yourGoals" Content="Your Goals" Width="60" Height="20" Canvas.Left="20" Canvas.Top="180" VerticalAlignment="Top" HorizontalAlignment="Left" Click="YourGoalsButtonClick"/>
    </Canvas>
</Window>
